{
    "crons": [
        {
            "path": "/api/cron",
            "schedule": "* * * * *"
        }
    ],
    "functions": {
        "api/cron.ts": {
            "maxDuration": 60
        },
        "api/index.ts": {
            "maxDuration": 30
        }
    },
    "rewrites": [
        {
            "source": "/api/(?!cron$|cron/).*",
            "destination": "/api/index.ts"
        }
    ]
}